{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Dev",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ],
      "program": "${workspaceFolder}/dist/main.js",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Production",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ],
      "program": "${workspaceFolder}/dist/main.js",
      "outputCapture": "std",
      "args": ["debug"],
      "env": {
        "NODE_ENV": "production"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Playground",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ],
      "program": "${workspaceFolder}/dist/playground.js",
      "outputCapture": "std"
    },
  ]
}
